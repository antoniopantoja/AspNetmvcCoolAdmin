
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="author" content="Hau Nguyen">
    <meta name="keywords" content="au theme template">

    <title>DESAFIO.NET</title>

    <link href="~/Content/bootstrapmin.css" rel="stylesheet" />
    <link href="~/Content/theme.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/Content/animsition/animsition.min.css" rel="stylesheet" />

    <meta name="robots" content="index, nofollow">
    <body class="animsition" style="animation-duration: 900ms; opacity: 1;">
        <div class="page-wrapper">

            <header class="header-mobile d-block d-lg-none">
                <div class="header-mobile__bar">
                    <div class="container-fluid">
                        <div class="header-mobile-inner">
                            <a class="logo" href="/Dashboard/Index">
                                <img src="~/Imagens/Logo/logo.png" alt="" />
                            </a>
                            <button class="hamburger hamburger--slider" type="button" style="font-size: 30px;">
                                <span class="fa fa-reorder">
                                </span>
                            </button>
                        </div>
                    </div>
                </div>
                <nav class="navbar-mobile">
                    <div class="container-fluid">
                        <ul class="navbar-mobile__list list-unstyled">
                            <li class="active">
                                <a href="/Dashboard/Index" style="text-decoration: none;">
                                    <i class="fa fa-dashboard"></i>Dashboard
                                </a>
                            </li>
                            <li>
                                <a href="/Cliente/Index" style="text-decoration: none;">
                                    <i class="fa fa-user"></i>Cadastro
                                </a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>

            <aside class="menu-sidebar d-none d-lg-block">
                <a class="logo" href="/Dashboard/Index">
                    <img src="~/Imagens/Logo/logo.png" alt="" />
                </a>
                <div class="menu-sidebar__content js-scrollbar1 ps">
                    <nav class="navbar-sidebar">
                        <ul class="list-unstyled navbar__list">
                            <li class="active">
                                <a href="/Dashboard/Index" style="text-decoration: none;">
                                    <i class="fa fa-dashboard"></i>Dashboard
                                </a>
                            </li>
                            <li>
                                <a href="/Cliente/Index" style="text-decoration: none;">
                                    <i class="fa fa-user"></i>Cadastro
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <div class="ps__rail-x" style="left: 0px; bottom: 0px;"><div class="ps__thumb-x" tabindex="0" style="left: 0px; width: 0px;"></div></div><div class="ps__rail-y" style="top: 0px; right: 0px; height: 285px;"><div class="ps__thumb-y" tabindex="0" style="top: 0px; height: 0px;"></div></div>
                </div>
            </aside>

            <div class="page-container">

                <header class="header-desktop">
                </header>


                <div class="main-content">
                    <div class="section__content section__content--p30">
                        @RenderBody()
                    </div>
                </div>


            </div>
        </div>

        <div class="modal show" id="sucesso" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="background-color: rgba(255, 255, 255, 0.35);" data-backdrop="static" data-keyboard="false">
            <div class="modal-dialog " role="document">
                <div class="alert alert-success"><strong>Positiva!</strong><p class="textoMsgModal"></p></div>
            </div>
        </div>

        <div class="modal show" id="erro" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="background-color: rgba(255, 255, 255, 0.35);">
            <div class="modal-dialog " role="document">
                <div class="alert alert-danger"><strong>Negativa!</strong><p class="textoMsgModal"></p></div>
            </div>
        </div>

        <!-- Main JS-->

        <script src="~/Scripts/jquery-3.4.1.js"></script>
        <script src="~/Scripts/animsition/animsition.min.js"></script>
        <script src="~/Scripts/main.js"></script>

    </body>
</html>


@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
